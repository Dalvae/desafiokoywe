generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Quote {
  id              String   @id @default(uuid())
  fromCurrency    String
  toCurrency      String
  amount          Float
  rate            Float
  convertedAmount Float
  createdAt       DateTime @default(now())
  expiresAt       DateTime
}
